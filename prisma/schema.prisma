generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pedido {
  codigo Int @id @unique
  dataAtual DateTime
  tipo String
  quantidade Int
  responsavel String
  situacao String
  horaInicio DateTime?
  horaPausa DateTime?
  horaReinicio DateTime?
  horaFinal DateTime?
  observacoes String?
  tempoProduzindo DateTime?
  tempoTotal DateTime?
}